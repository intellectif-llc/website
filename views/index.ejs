    <style>
        .modal {
            display: none;
            position: fixed;
            z-index: 1;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            overflow: auto;
            background-color: rgba(0, 0, 0, 0.7);
            align-items: center;
            /* Vertical center */
            justify-content: center;
            /* Horizontal center */
        }

        /* Modal content */
        .modal-content {
            background-color: #ffffff;
            padding: 20px;
            border-radius: 10px;
            width: 80%;
            max-width: 600px;
            position: relative;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.3);
            /* Add a shadow for better visibility */
            animation: modalFadeIn 0.3s ease;
            /* Optional: animation for a smoother effect */
        }

        /* Close button */
        .close-modal {
            position: absolute;
            right: 20px;
            top: 10px;
            font-size: 24px;
            font-weight: bold;
            cursor: pointer;
        }

        /* Modal title */
        #modalTitle {
            color: #051028;
        }

        /* Modal description */
        #modalDescription {
            color: #333333;
            font-size: 16px;
        }

        /* Optional: Fade-in animation for a smoother modal appearance */
        @keyframes modalFadeIn {
            from {
                opacity: 0;
                transform: scale(0.9);
            }

            to {
                opacity: 1;
                transform: scale(1);
            }
        }
    </style>
</head>

