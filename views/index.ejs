                    y: 70,
                    opacity: 0
                });

                gsap.to(["#vertical-bar1", "#vertical-bar2", "#vertical-bar3", "#vertical-bar4", "#vertical-bar5"], {
                    y: 0,
                    opacity: 1,
                    duration: 1,
                    stagger: {
                        each: 0.3, // Time between bars
                    },
                    repeat: -1, // Loop infinitely
                    repeatDelay: 3, // Stay visible for 3 seconds
                });

                // Animate components on scroll
                const components = [
                    'calendar', 'checklist', 'development', 'completed-project',
                    'description1', 'description2', 'description3', 'description4', 'check'
                ];

                components.forEach((id) => {
                    gsap.from(`#${id}`, {
                        opacity: 0,
                        scale: 0.8,
                        duration: 1,
                        scrollTrigger: {
                            trigger: `#${id}`,  // Trigger animation when the component is in view
                            start: "top 80%",    // Start when top of the element hits 80% of viewport
                            end: "top 65%",      // End at 65% of viewport height
                            scrub: true,         // Smoothly transition with scrolling
                            toggleActions: "play reverse play reverse",  // Play forward, reverse when scrolling back
                        },
                    });
                });

                // Define the heading IDs you want to animate
                const headings = ['#service-list', '#smooth-process', '#faqs', '#tech-stack'];

                // Loop through each heading and apply the animation separately
                headings.forEach((heading) => {
                    gsap.from(heading, {
                        y: -100, // Slide in from the left
                        scale: 0.8, // Add a slight scaling effect
                        opacity: 0, // Fade in from 0 opacity
                        duration: 1.8, // Animation duration
                        ease: "power4.out", // Interesting easing effect for a smoother finish
                        scrollTrigger: {
                            trigger: heading, // Individual scroll trigger for each element
                            start: "top 90%", // Start when top of the element reaches 90% of viewport height
                            end: "top 70%", // End at 70%
                            scrub: true, // Smooth transition with scroll
                            toggleActions: "play none none reverse", // Play forward on scroll, reverse on scroll up
                        },
                    });
                });



                // Text animation that alternates between original and new text
                gsap.to("#main-title", {
                    duration: 2,
                    text: "Boost your profits with Innovative Tech Solutions",
                    delay: 6,
                    repeat: -1,   // Infinite loop
                    yoyo: true,   // Reverse to original text
                    repeatDelay: 6,  // Pause for 6 seconds before reversing
                });

                // Fade animation for #check
                gsap.to("#check", {
                    opacity: 0,
                    duration: 2,
                    repeat: -1,
                    delay: 6,
                    ease: "linear",
                });

                //accordion animation
                // Initialize accordion functionality
                const accordionGroups = document.querySelectorAll(".accordion-group");

                accordionGroups.forEach(group => {
                    const menu = group.querySelector(".accordion-menu");
                    const content = group.querySelector(".accordion-content");
                    const plusIcon = group.querySelector(".accordion-plus");

                    // Set initial states
                    gsap.set(content, { height: 0, opacity: 0 });

                    // Toggle accordion on click
                    menu.addEventListener("click", () => {
                        const isOpen = gsap.getProperty(content, "height") !== 0;

                        // Close all other accordion items
                        accordionGroups.forEach(otherGroup => {
                            if (otherGroup !== group) {
                                gsap.to(otherGroup.querySelector(".accordion-content"), { height: 0, opacity: 0, duration: 0.3 });
                                gsap.to(otherGroup.querySelector(".accordion-plus"), { rotate: 0, duration: 0.5, ease: "power3.inOut" });
                            }
                        });

                        // Toggle current accordion item
                        if (!isOpen) {
                            gsap.to(content, { height: "auto", opacity: 1, duration: 0.3, ease: "power1.out" });
                            gsap.to(plusIcon, { rotate: 45, duration: 0.5, ease: "power3.inOut" });
                        } else {
                            gsap.to(content, { height: 0, opacity: 0, duration: 0.3 });
                            gsap.to(plusIcon, { rotate: 0, duration: 0.5, ease: "power3.inOut" });
                        }
                    });
                });

                //Dynamically calculate width
                document.addEventListener('DOMContentLoaded', function () {
                    const slider = document.querySelector('.tech-stack-slider');
                    const images = slider.querySelectorAll('.tech-stack');

                    // Clone the images and append them to the slider
                    images.forEach(img => {
                        const clone = img.cloneNode(true);
                        slider.appendChild(clone);
                    });

                    // Calculate the total width of all images
                    let totalWidth = 0;
                    slider.querySelectorAll('.tech-stack').forEach(img => {
                        totalWidth += img.offsetWidth + parseInt(window.getComputedStyle(img).marginLeft) +
                            parseInt(window.getComputedStyle(img).marginRight);
                    });

                    // Set the width of the slider
                    slider.style.width = `${totalWidth / 2}px`;

                    // Pause animation on hover
                    slider.addEventListener('mouseenter', () => {
                        slider.style.animationPlayState = 'paused';
                    });

                    slider.addEventListener('mouseleave', () => {
                        slider.style.animationPlayState = 'running';
                    });

                    // Reset animation when it completes
                    slider.addEventListener('animationiteration', () => {
                        slider.style.animation = 'none';
                        slider.offsetHeight; // Trigger reflow
                        slider.style.animation = null;
                    });
                });

                /* Modal logic and details */
                const serviceDetails = {
                    "web-development": {
                        title: "Web Development",
                        description: "We design and develop responsive, user-friendly websites that not only look great but also deliver seamless user experiences. Whether you need an informational site, an e-commerce platform, or a custom web application, we tailor our solutions to meet your specific business objectives."
                    },
                    "backend-development": {
                        title: "Backend Development",
                        description: "Our robust backend development services utilize Node.js, Express, and various databases to power your applications. We specialize in integrating business processes through APIs, enabling seamless data flow between your systems and third-party services to enhance efficiency and scalability."
                    },
                    "aws-cloud": {
                        title: "AWS Cloud Services",
                        description: "Leverage our expertise in AWS to deploy, manage, and scale your applications in the cloud. Utilizing AWS tools like EC2, S3, Lambda, and RDS, we ensure your applications are reliable, secure, and capable of handling growing workloads, providing the reliability your business demands."
                    },
                    "digital-marketing": {
                        title: "Digital Marketing",
                        description: "Boost your brand's online presence with our advanced digital marketing strategies. We specialize in integrating Meta tools like the Facebook Conversion API and leveraging Meta's suite of marketing platforms to optimize your advertising campaigns, increase engagement, and drive measurable results."
                    },
                    "mobile-development": {
                        title: "Mobile App Development",
                        description: "We develop high-quality, user-centric mobile applications for both iOS and Android platforms. By utilizing native development and cross-platform frameworks like React Native and Flutter, we ensure your app delivers exceptional performance and reaches a wider audience."
                    },
                    "ai-business": {
                        title: "AI for Your Business",
                        description: "Transform your operations with our AI and Machine Learning solutions. We help automate processes, gain actionable insights from data, and improve decision-making by integrating AI technologies tailored to your specific needs, driving innovation and efficiency in your business."
                    }
                };


                // Function to open modal with specific content
                function openModal(serviceId) {
                    const modal = document.getElementById('serviceModal');
                    const modalTitle = document.getElementById('modalTitle');
                    const modalDescription = document.getElementById('modalDescription');

                    // Set the modal title and description based on the clicked service
                    if (serviceDetails[serviceId]) {
                        modalTitle.textContent = serviceDetails[serviceId].title;
                        modalDescription.textContent = serviceDetails[serviceId].description;
                    }

                    // Display the modal
                    modal.style.display = 'flex';

                    // Optional: Add a slight fade-in effect
                    modal.classList.add('fade-in');
                }

                // Function to close modal
                function closeModal() {
                    document.getElementById('serviceModal').style.display = 'none';
                }

                // Event listener to close modal when close button is clicked
                document.querySelector('.close-modal').addEventListener('click', closeModal);

                // Event listeners for each service item
                document.querySelectorAll('.category-item').forEach(item => {
                    item.addEventListener('click', function () {
                        const serviceId = this.getAttribute('data-service');
                        openModal(serviceId);
                    });
                });

                // Event listener to close modal when clicking outside the modal content
                window.addEventListener('click', function (event) {
                    const modal = document.getElementById('serviceModal');
                    if (event.target === modal) {
                        closeModal();
                    }
                });
                /* End of modal logic and details */
            </script>

            <script src="/js/main.js"></script>

</body>

