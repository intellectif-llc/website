<script src="https://cdn.jsdelivr.net/npm/gsap@3.12.5/dist/TextPlugin.min.js"></script>

    <script>
const toggleButton = document.querySelector('.menu-btn');
const menuContainer = document.querySelector('.menu');
const menuItems = document.querySelectorAll('.menu-item');
let menuOpen = false;

if (toggleButton) {
    toggleButton.addEventListener('click', () => {
        menuOpen = !menuOpen;
        toggleButton.classList.toggle('open');

        if (menuOpen) {
            // Open menu
            menuContainer.classList.add('active');
            // Delay adding 'active' class to menu items
            setTimeout(() => {
                menuItems.forEach((item, index) => {
                    setTimeout(() => {
                        item.classList.add('active');
                    }, index * 50); // Stagger item appearance
                });
            }, 200); // Delay after background appears
        } else {
            // Close menu
            // Remove 'active' class from menu items with a staggered delay
            menuItems.forEach((item, index) => {
                setTimeout(() => {
                    item.classList.remove('active');
                }, index * 50); // Stagger item disappearance
            });
            
            // Delay removing 'active' class from menu container
            setTimeout(() => {
                menuContainer.classList.remove('active');
            }, menuItems.length * 50 + 100); // Delay before menu slides up
        }
    });
}

// Register GSAP plugins
gsap.registerPlugin(ScrollTrigger, ScrollToPlugin, TextPlugin);

// Rotate gears indefinitely
gsap.to("#gear1,#gear2", {
  rotation: 360,
  duration: 5,
  repeat: -1,
  ease: "linear",
  transformOrigin: "50% 50%",
});

// Fade out 'idea sparks' repeatedly
gsap.to("#idea-sparks", {
  opacity: 0,
  duration: 1.5,
  repeat: -1,
  ease: "linear",
});

// Staggered bar chart animation
gsap.set("#vertical-bar1, #vertical-bar2, #vertical-bar3, #vertical-bar4, #vertical-bar5", {
  y: 70,
  opacity: 0
});

gsap.to(["#vertical-bar1", "#vertical-bar2", "#vertical-bar3", "#vertical-bar4", "#vertical-bar5"], {
  y: 0,
  opacity: 1,
  duration: 1,
  stagger: {
    each: 0.3, // Time between bars
  },
  repeat: -1, // Loop infinitely
  repeatDelay: 3, // Stay visible for 3 seconds
});

// Animate components on scroll
const components = [
  'calendar', 'checklist', 'development', 'completed-project',
  'description1', 'description2', 'description3', 'description4','check'
];

components.forEach((id) => {
  gsap.from(`#${id}`, {
    opacity: 0,
    scale: 0.8,
    duration: 1,
    scrollTrigger: {
      trigger: `#${id}`,  // Trigger animation when the component is in view
      start: "top 80%",    // Start when top of the element hits 80% of viewport
      end: "top 65%",      // End at 65% of viewport height
      scrub: true,         // Smoothly transition with scrolling
      toggleActions: "play reverse play reverse",  // Play forward, reverse when scrolling back
    },
  });
});

// Text animation that alternates between original and new text
gsap.to("#main-title", {
  duration: 2,
  text: "Boost your business profits with Innovative Tech Solutions",
  delay: 6,
  repeat: -1,   // Infinite loop
  yoyo: true,   // Reverse to original text
  repeatDelay: 6,  // Pause for 6 seconds before reversing
});


gsap.to("#check", {
  opacity: 0,
  duration: 2,
  repeat: -1,
  delay:6,
  ease: "linear",
});

// Animation for the check element inside the calendar, triggered by scroll
